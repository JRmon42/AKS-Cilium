# Example terraform.tfvars file
# Copy this file to terraform.tfvars and modify as needed

resource_group_name = "rg-aks-cilium-demo"
location            = "eastus"
cluster_name        = "aks-cilium-demo"
kubernetes_version  = "1.28"
node_count          = 3
node_vm_size        = "Standard_D4s_v3"
enable_auto_scaling = true
min_count           = 2
max_count           = 5

tags = {
  Environment = "Demo"
  Project     = "AKS-Cilium"
  ManagedBy   = "Terraform"
  Owner       = "YourName"
}
